<h2 style="color:crimson;">Vista de Productos</h2>
{{#if listExists}}
    <div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Nombre</th>
            <th>Precio</th>
            <th>Link URL</th>
        </tr>
        {{#each productos}}
            <tr>
            <td>
                {{this.nombre}}
            </td>
            <td>
                {{this.precio}}
            </td>
            <td>
                <img src="{{this.url}}" width="60" height="60">
            </td>
            
        </tr>
        {{/each}}
        
    </table>
</div>
   
    {{else}}
    <h3 class="alert alert-danger">no se encontraron datos</h1>
    
{{/if}}
</h3>

<button onclick="location.href = '/'"  class="btn btn-success mt-3 mb-5" >Home</button>